{
  "name": "sweet-narcisse",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:https": "cross-env NEXT_DEV_HOSTNAME=sweet.local next dev --turbo --hostname sweet.local --port 3000 --experimental-https --experimental-https-key certs/dev-key.pem --experimental-https-cert certs/dev-cert.pem",
    "dev:webpack": "cross-env TURBOPACK=0 next dev",
    "build": "next build",
    "build:mobile": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:unused": "ts-prune | grep -v '(used in module)' | grep -v '\\.next' | grep -v 'tests/' | grep -v 'default$' | grep -v 'route\\.ts'",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "export": "next export",
    "generate:icons": "node scripts/generate-icons.cjs",
    "release": "node scripts/release-version.js",
    "optimize:images": "node scripts/convert-images.js",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:mobile": "node scripts/lighthouse-audit.js --mobile",
    "cap:sync": "npx cap sync",
    "cap:sync:dev": "cross-env CAP_USE_DEV=true npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android"
